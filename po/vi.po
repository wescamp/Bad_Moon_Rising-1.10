#. [unit]: type=Ukian Sick, id=Carghanna
#: Bad_Moon_Rising/scenarios3/2_05_Tien.cfg:395
#: Bad_Moon_Rising/scenarios3/2_01_Trouble.cfg:162
#: Bad_Moon_Rising/scenarios3/2_07_Salvation.cfg:278
#: Bad_Moon_Rising/scenarios3/2_07_Salvation.cfg:525
#: Bad_Moon_Rising/scenarios3/2_07_Salvation.cfg:936
#: Bad_Moon_Rising/scenarios3/2_07_Salvation.cfg:1130
#: Bad_Moon_Rising/scenarios3/2_05_Blind.cfg:222
#: Bad_Moon_Rising/scenarios3/2_05_Blind.cfg:533
#: Bad_Moon_Rising/scenarios3/2_04_River.cfg:582
#: Bad_Moon_Rising/scenarios3/2_04_River.cfg:596
#: Bad_Moon_Rising/scenarios3/2_04_Checkpoint.cfg:111
#: Bad_Moon_Rising/scenarios3/2_07_Volcano.cfg:325
msgid "Carghanna"
msgstr ""

#. [side]: type=Primevalist Monk, id=Asher
#: Bad_Moon_Rising/scenarios3/2_01_Trouble.cfg:179
msgid "Asher"
msgstr ""

#. [unit]: type=Primevalist Fighter, id=Skulker
